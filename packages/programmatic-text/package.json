        "ts-jest": "^28.0.5",
        "ts-node": "^10.8.1",
        "tslib": "^2.4.0",
        "ttypescript": "^1.5.13",
        "typescript": "^4.7.4",
        "typescript-transform-paths": "^3.3.1"
    }
